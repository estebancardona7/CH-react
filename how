[1mdiff --git a/src/components/Counter/Counter.jsx b/src/components/Counter/Counter.jsx[m
[1mnew file mode 100644[m
[1mindex 0000000..73b2d03[m
[1m--- /dev/null[m
[1m+++ b/src/components/Counter/Counter.jsx[m
[36m@@ -0,0 +1,33 @@[m
[32m+[m[32mimport { useState } from "react";[m
[32m+[m
[32m+[m[32mconst Counter = () => {[m
[32m+[m[32m  const [counter, setCounter] = useState(200);[m
[32m+[m[32m  const [user, setUser] = useState("");[m
[32m+[m
[32m+[m[32m  const sumar = () => {[m
[32m+[m[32m    setCounter(counter + 1);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const restar = () => {[m
[32m+[m[32m    setCounter(counter - 1);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const login = () => {[m
[32m+[m[32m    setUser("pepito");[m
[32m+[m[32m  };[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div>[m
[32m+[m[32m      <h1>Este es el contador</h1>[m
[32m+[m[32m      <button onClick={restar}>Restar</button>[m
[32m+[m[32m      <h3>{counter}</h3>[m
[32m+[m[32m      <button onClick={sumar}>Sumar</button>[m
[32m+[m[32m      <br />[m
[32m+[m[32m      <br />[m
[32m+[m[32m      <button onClick={login}>Login</button>[m
[32m+[m
[32m+[m[32m      {user.length > 0 ? <h2>Hola chaval {user}</h2> : <h2>Logeate wachin</h2>}[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport default Counter;[m
